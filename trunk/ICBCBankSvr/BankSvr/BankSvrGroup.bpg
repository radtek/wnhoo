#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = BankSvr.exe BankSvrClient.exe 
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

BankSvr.exe: BankSvr.dpr
  $(DCC)

BankSvrClient.exe: BankSvrClient.dpr
  $(DCC)
